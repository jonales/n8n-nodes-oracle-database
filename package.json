{
  "name": "@jonales/n8n-nodes-oracle-database",
  "type": "commonjs",
  "version": "1.0.7",
  "description": "Oracle Database node com recursos avançados para cargas pesadas e Oracle 19c+. Suporte completo para PL/SQL, bulk operations, connection pooling e Oracle Cloud Infrastructure.",
  "keywords": [
    "n8n-community-node-package",
    "oracle"
  ],
  "license": "MIT",
  "homepage": "https://github.com/jonales/n8n-nodes-oracle-database",
  "author": {
    "name": "Jônatas Meireles Sousa Vieira",
    "email": "jonatas.mei@outlook.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonales/n8n-nodes-oracle-database.git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts,.js --cache",
    "prebuild": "rimraf dist",
    "build": "tsc && gulp build:icons",
    "verify": "node -e \"const fs=require('fs'); const files=['dist/credentials/Oracle.credentials.js','dist/nodes/Oracle/OracleDatabase.node.js']; files.forEach(f=>{ if(!fs.existsSync(f)) throw new Error('Missing: '+f); });\"",
    "prepublishOnly": "npm run build && npm run verify"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/Oracle.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Oracle/OracleDatabase.node.js",
      "dist/nodes/Oracle/OracleDatabaseAdvanced.node.js",
      "dist/nodes/Oracle/OracleVectorStore.node.js",
      "dist/nodes/Oracle/ChatMemory.node.js"
    ]
  },
  "dependencies": {
    "oracledb": "^6.9.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.23",
    "@types/node": "^18.19.130",
    "@types/oracledb": "^5.3.1",
    "gulp": "^5.0.1",
    "n8n-core": "^1.0.0",
    "n8n-workflow": "^1.0.0",
    "rimraf": "^6.0.1",
    "typescript": "~5.2.0"
  },
    "peerDependencies": {
      "oracledb": ">=6.0.0"
    },
    "peerDependenciesMeta": {
      "oracledb": {
        "optional": false
      }
    }
}
